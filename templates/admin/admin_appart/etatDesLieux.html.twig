        $('#wizardForm7').attr('data-wizard-state', 'current');
        // form a cacher
        $('#wizard-step6').attr('data-wizard-state', 'done');
        $('#wizardForm6').attr('data-wizard-state', 'done');
        return;
    }

    //si on est sur le slide 68 on cache la formulaire 6 au profit du 7
    if ($('.pb-5[data-wizard-state="current"]').attr('id') == "wizardForm8Slide90"){
        // form a afficher
        $('#wizard-step8').attr('data-wizard-state', 'current');
        $('#wizardForm8').attr('data-wizard-state', 'current');
        // form a cacher
        $('#wizard-step7').attr('data-wizard-state', 'done');
        $('#wizardForm7').attr('data-wizard-state', 'done');
        return;
    }

    //on recupere l'id du prochain element et on ajoute la classe active au btn actuel
    var state = $('.element'+idSlideNext++).attr("data-btn-id");
    $('#'+ state).addClass("active")

}

function formPrecedentSlide() {
    if ($('#wizardForm1Slide1').attr("data-wizard-state") === "current") {
        $('#wizardForm1').attr('data-wizard-state', 'current');
        $('#wizardForm1Slide1').attr('data-wizard-state', 'done');
        return;
    }

    idSlidePrecedent = parseInt($('.pb-5[data-wizard-state="current"]').attr('data-wizard-id'));
    idSlideNext = idSlidePrecedent - 1;

    $('.pb-5[data-wizard-id='+idSlideNext+']').attr('data-wizard-state', 'current');
    $('.pb-5[data-wizard-id='+idSlidePrecedent+']').attr('data-wizard-state', 'done');

    //on recupere l'id du prochain element et on ajoute la classe active au btn actuel
    var state = $('.element'+idSlideNext).attr("data-btn-id");
    $('#'+ state).addClass("active")
}

</script>

